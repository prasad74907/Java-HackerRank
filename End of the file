import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args)
    {
        int count=0;
        Scanner s=new Scanner(System.in);
        while(s.hasNext())
        {
            count++;
            String str=s.nextLine();
            System.out.println(count+" "+str);
        }
        
    }
}
